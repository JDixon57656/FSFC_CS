import java.util.*;
public class scanners {
	public static void main(String[] args){
		checkEligibility ();
	}
	static void checkEligibility (){
		int age;
		
		
		
		do{
			Scanner sc=new Scanner(System.in);
		System.out.print("What is your age?");
		age=sc.nextInt();
			System.out.println("Congeratulations on being" + " " + age );
		}
			while (age<=200);

	}
}